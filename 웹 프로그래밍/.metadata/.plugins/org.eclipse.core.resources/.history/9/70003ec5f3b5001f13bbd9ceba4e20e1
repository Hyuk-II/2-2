	<%
	Cookie[] cookies = request.getCookies();
	
	String cName;
	String cValue;
	
	for(Cookie cookie : cookies) {
		cName = cookie.getName();
		cValue = cookie.getValue();
	%>
	<%=cName %> : <%= cValue %>
	<%
	}
	%>
